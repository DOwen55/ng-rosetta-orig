{
  "name": "fruit20",
  "version": "0.1.2",
  "author": "David Owen",
  "description": "Angular2 Reference Application (ng-rosetta series)",
  "license": "BSD3",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start config/karma.conf.js",
    "build": "rimraf dist && webpack --config config/webpack.dev.js --progress --profile --bail",
    "postinstall": "typings install",
    "copyresources": "copyfiles node_modules/bootstrap/dist/css/bootstrap.min.css node_modules/core-js/client/shim.min.js node_modules/zone.js/dist/zone.min.js dist/assets -u",
    "clean:aot": "rimraf aot",
    "build:aot1": "ngc -p tsconfig-aot.1.json",
    "build:aot2": "ngc -p tsconfig-aot.2.json",
    "build:aot3": "rollup -c rollup-config.js",
    "build:aot4": "tsc -p tsconfig-es5.json",
    "serve:aot": "lite-server -c config/lite-server-config.json",
    "build:aot": "echo \"Clean the AOT directory.\" && npm run clean:aot && echo \"Run the AOT 1st pass.\" && npm run build:aot1 && echo \"Run the AOT 2nd pass.\" && npm run build:aot2 && echo \"Run the RollupJS task.\" && npm run build:aot3 && echo \"Run the ES5 Transpiler Task.\" && npm run build:aot4 && echo \"Launch the server.\" && npm run serve:aot"
  },
  "dependencies": {
    "@angular/common": "4.0.1",
    "@angular/compiler": "4.0.1",
    "@angular/compiler-cli": "4.0.1",
    "@angular/core": "4.0.1",
    "@angular/forms": "4.0.1",
    "@angular/http": "4.0.1",
    "@angular/platform-browser": "4.0.1",
    "@angular/platform-browser-dynamic": "4.0.1",
    "@angular/platform-server": "4.0.1",
    "@angular/router": "4.0.1",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.15",
    "bootstrap": "4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "angular2-router-loader": "0.3.5",
    "angular2-template-loader": "0.6.2",
    "css-loader": "0.28.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "jasmine-core": "^2.4.1",
    "karma": "1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-istanbul": "^0.4.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "2.0.1",
    "phantomjs-prebuilt": "^2.1.14",
    "raw-loader": "0.5.1",
    "rimraf": "^2.5.2",
    "rollup": "^0.37.0",
    "rollup-plugin-commonjs": "^6.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-postcss": "^0.2.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",    
    "script-loader": "0.7.0",
    "style-loader": "0.16.1",
    "ts-loader": "2.0.3",
    "typescript": "2.3.0",
    "typings": "^1.0.4",
    "webpack": "2.3.3",
    "webpack-browser-plugin": "^1.0.20",
    "webpack-dev-server": "2.4.2",
    "webpack-merge": "4.1.0"
  },
  "main": "index.js"
}
