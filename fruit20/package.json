{
  "name": "fruit20",
  "version": "0.1.0",
  "author": "David Owen",
  "description": "Angular2 Reference Application (ng-rosetta series)",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start config/karma.conf.js",
    "build": "rimraf dist && webpack --config config/webpack.dev.js --progress --profile --bail",
    "postinstall": "typings install",
    "copyresources": "copyfiles node_modules/bootstrap/dist/css/bootstrap.min.css node_modules/core-js/client/shim.min.js node_modules/zone.js/dist/zone.min.js dist/assets -u",
    "clean:aot": "rimraf aot",    
    "build:aot1": "ngc -p tsconfig-aot.1.json",
    "build:aot2": "ngc -p tsconfig-aot.2.json",
    "build:aot3": "rollup -c rollup-config.js",
    "build:aot4": "tsc -p tsconfig-es5.json",
    "serve:aot": "lite-server -c config/lite-server-config.json",
    "build:aot": "echo \"Clean the AOT directory.\" && npm run clean:aot && echo \"Run the AOT 1st pass.\" && npm run build:aot1 && echo \"Run the AOT 2nd pass.\" && npm run build:aot2 && echo \"Run the RollupJS task.\" && npm run build:aot3 && echo \"Run the ES5 Transpiler Task.\" && npm run build:aot4 && echo \"Launch the server.\" && npm run serve:aot"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.4.3",
    "@angular/compiler": "2.4.3",
    "@angular/compiler-cli": "2.4.3",
    "@angular/core": "2.4.3",
    "@angular/forms": "2.4.3",
    "@angular/http": "2.4.3",
    "@angular/platform-browser": "2.4.3",
    "@angular/platform-browser-dynamic": "2.4.3",
    "@angular/platform-server": "^2.4.3",
    "@angular/router": "3.4.3",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.15",
    "bootstrap": "4.0.0-alpha.5",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "angular2-router-loader": "^0.3.4",
    "angular2-template-loader": "^0.4.0",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "jasmine-core": "^2.4.3",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "rollup": "^0.37.0",
    "rollup-plugin-commonjs": "^6.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-postcss": "^0.2.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1",
    "ts-loader": "1.3.1",
    "typescript": "2.0.10",
    "typings": "2.0.0",
    "webpack": "^1.13.3",
    "webpack-browser-plugin": "^1.0.20",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^1.0.2",
    "webpack-uglify-js-plugin": "^1.1.9"
  },
  "main": "index.js"
}
